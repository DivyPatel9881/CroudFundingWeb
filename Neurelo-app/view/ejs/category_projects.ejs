<%- include("header"); %>
	<link rel="stylesheet" href="/category_projects.css">
	<div class="container">
		<div class="jumbotron">
			<h1 class="header"><%= category %></h1>
		</div>
		<hr>
		<div class="row flex">
			<% projects.forEach(function(project){ %>
				<div class="col-lg-3 col-md-4 col-sm-6 col-xs-12 ">
					<div class="thumbnail">
						<img src="<%= project.photo %>"/>
						<p class="cust"><%= project.name %></p>
						<a href="/projects/know-more/<%= project._id %>"><button class="btn btn-primary btn-md">Know More</button></a>
					</div>
				</div>
			<% }) %>
		</div>
	</div>
<%- include("footer"); %>